{
  "name": "@ahoo-wang/fetcher-decorator",
  "version": "3.10.7",
  "description": "TypeScript decorators for clean, declarative API service definitions with Fetcher HTTP client. Enables automatic parameter binding, method mapping, and type-safe API interactions.",
  "keywords": [
    "fetch",
    "http",
    "client",
    "decorator",
    "typescript",
    "api",
    "service",
    "declarative",
    "type-safe"
  ],
  "author": "Ahoo-Wang",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Ahoo-Wang/fetcher/tree/master/packages/fetcher-decorator",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ahoo-Wang/fetcher.git",
    "directory": "packages/fetcher-decorator"
  },
  "bugs": {
    "url": "https://github.com/Ahoo-Wang/fetcher/issues"
  },
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test:ui": "vitest --ui",
    "test": "vitest run --coverage",
    "lint": "eslint . --fix",
    "clean": "rm -rf dist",
    "analyze": "npx vite-bundle-analyzer -p auto"
  },
  "peerDependencies": {
    "@ahoo-wang/fetcher": "workspace:^3.0.0"
  },
  "dependencies": {
    "reflect-metadata": "catalog:"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "globals": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "unplugin-dts": "catalog:",
    "vite": "catalog:",
    "vite-bundle-analyzer": "catalog:",
    "vitest": "catalog:",
    "msw": "catalog:"
  }
}
